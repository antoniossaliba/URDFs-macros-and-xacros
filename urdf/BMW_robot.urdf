<?xml version="1.0"?>
<robot name="ANTONIOS_car">

    <link name="base_link">
        <visual>
            <origin xyz="0 0 0.05" rpy="0 0 0" />
            <geometry>
                <box size="0.5 0.3 0.1" />
            </geometry>
            <material name="blue">
                <color rgba="0.0 0.0 1.0 1.0" />
